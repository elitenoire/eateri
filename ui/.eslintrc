{
	"plugins": ["react-perf"],
	"extends": ["wesbos", "plugin:react-perf/recommended"],
	"rules": {
		"react/prop-types": [2, { "skipUndeclared": true }],
		"react/jsx-props-no-spreading": 0,
		"prettier/prettier": [
			"error",
			{
				"trailingComma": "es5",
				"singleQuote": true,
				"printWidth": 120,
				"tabWidth": 4,
				"semi": false
			}
		]
	},
	"settings": {
		"import/resolver": {
			"babel-module": {}
		}
	}
}
