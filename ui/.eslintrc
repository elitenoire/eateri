{
	"extends": ["wesbos"],
	"rules": {
		"react/prop-types": [0],
		"prettier/prettier": [
			"error",
			{
				"trailingComma": "es5",
				"singleQuote": true,
				"printWidth": 120,
				"tabWidth": 4,
				"semi": false
			}
		]
	},
	"settings": {
		"import/resolver": {
			"babel-module": {}
		}
	}
}
