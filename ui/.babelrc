{
	"presets": ["next/babel"],
	"plugins": [
		[
			"module-resolver",
			{
				"root": ["./"],
				"alias": {
					"~components": "./components",
					"~static": "./static",
					"~lib": "./lib",
					"~context": "./context",
					"~theme": "./theme"
				}
			}
		],
		[
			"styled-components",
			{
				"ssr": true,
				"displayName": true
			}
		]
	]
}
