{
	"workbench.colorCustomizations": {
		"tab.unfocusedActiveBorder": "#fff0"
	},
	"eslint.workingDirectories": [
		{
			"directory": "./ui",
			"changeProcessCWD": true
		},
		{
			"directory": "./cms",
			"changeProcessCWD": true
		}
	],
	"eslint.options": {
		"ignorePath": ".gitignore"
	},
	"[javascript]": {
		"editor.formatOnSave": false
	},
	"[javascriptreact]": {
		"editor.formatOnSave": false
	},
	"editor.codeActionsOnSave": {
		"source.fixAll": true
	},
	"prettier.disableLanguages": ["javascript", "javascriptreact"],
	"eslint.alwaysShowStatus": true,
	"javascript.validate.enable": false,
	"svg.preview.background": "transparent",
	"svg.minify.cleanupIDs": false,
	"svg.minify.convertStyleToAttrs": false,
	"svg.minify.inlineStyles": false,
	"svg.minify.prefixIds": false,
	"svg.minify.removeTitle": false,
	"svg.preview.autoShow": true
}
